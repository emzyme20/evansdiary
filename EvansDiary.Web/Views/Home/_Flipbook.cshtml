@model IEnumerable<FlipbookItemViewModel>

@foreach (var item in Model)
{
    <img src="/Content/images/blank.jpg" class="dynamic_image rotating-item"
         data-src="@(item.Image.Source)"
         data-width="380" data-height="380"
         data-crop="fill" data-gravity="@(item.Crop ?? "face")" data-quality="80" data-fetch-format="auto" alt="@item.Image.Caption" />
    @*<img class="rotating-item" src="@item.Image.GetUrl(380.00, 380.00, 80, item.Crop ?? "g_face")" alt="@item.Image.Caption" />*@
}
